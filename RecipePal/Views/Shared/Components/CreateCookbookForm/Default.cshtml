@model Cookbook

<h3 class="text-center">Create a new cookbook!</h3>
<form method="post">
    <div class="form-group">
        <label asp-for="Title"></label>
        <input asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CuisineType"></label>
        <input asp-for="CuisineType" class="form-control" />
    </div>
    <input hidden asp-for="ChefId" value="@Model.Chef.Id" />
    <input type="submit" class="btn btn-outline-primary rounded-pill m-1" />
</form>